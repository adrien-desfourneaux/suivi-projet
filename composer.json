{
    "name": "adrien-desfourneaux/suivi-projet",
    "description": "Application ZF2 de suivi de projet",
    "license": "MIT",
    "keywords": [
        "framework",
        "zf2",
        "suivi",
        "projet"
    ],
    "homepage": "http://suivi-projet.com/",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.2.*",
        "doctrine/doctrine-orm-module": "0.*",
        "zf-commons/zfc-user": "dev-master"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "phpunit/php-code-coverage": ">=1.2.10,<1.3.0",
        "phpspec/phpspec": "2.0.*@dev",
        "henrikbjorn/phpspec-code-coverage" : "dev-master",
        "phpdocumentor/phpdocumentor": "2.*",
        "squizlabs/php_codesniffer": "1.*",
        "phpmd/phpmd" : "1.4.*",
        "zendframework/zftool": "dev-master",
        "zendframework/zend-developer-tools" : "dev-master",
        "bjyoungblood/bjy-profiler" : "~1.0",
        "ocramius/ocra-service-manager" : "~2.0"
    },
    "autoload": {
        "psr-0": {
          "DzProject": "module/DzProject/src",
          "Application": "module/Application/src"
        }
    }
}
